{
    "name": "Opendaje/ckan-php-api-client",
    "type": "Library",
    "description": "Provides an API consumer for the Ckan ReST API.",
    "homepage": "http://www.example.com",
    "authors": [
        {
            "name": "Zerai Teclai",
            "email": "teclaizerai@googlemail.com",
            "homepage": "http://www.example.com"
        }
    ],
    "keywords": ["REST","PHP", "CKAN", "ckan api consumer"],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle-services": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.10"
    },
    "autoload": {
        "psr-4": {
            "OpenDaje\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenDaje\\Tests\\": "tests/"
        }
    }
}
